/**app.wxss**/
@import 'style/weui.wxss';
@import 'style/common.wxss';
/* 缺省页 */
.noarea {
    width: 750rpx;
    height: 100%;
    box-sizing: border-box;
    padding-top: 12rpx;
    padding-bottom: 90rpx;
    background: #f7f8f9;
    
  }
  .noapplylist {
    width: 750rpx;
    height: 586rpx;
    background: #fff;
    font-size: 28rpx;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
  }
  .nodefault {
    width: 338rpx;
    height: 246rpx;
  }
  .nodefault .default-img {
    width: 338rpx;
    height: 246rpx;
  }
  .tiptext {
    margin-top: 40rpx;
  }
  .tosee-bor {
    line-height: 48rpx;
    text-align: center;
    font-size: 28rpx;
    width: 150rpx;
    height: 48rpx;
    border-radius: 24rpx;
    border: 2rpx solid #01c1a7;
    margin-top: 20rpx;
    color: #01c1a7;
  }
  
  page {
    font-weight: 400;
    background: #f7f8f9;
    width: 750rpx;
    box-sizing: border-box;
  }
  
  .img-right {
    display: inline-block;
    width: 20rpx;
    height: 23rpx;
  }
  /* 价格 */
  .text-noprice {
    color: #df5252;
    font-weight: 500;
    font-size: 36rpx;
  }
  
  .text-price {
    color: #df5252;
    font-weight: 500;
    font-size: 36rpx;
  }
  
  .text-price::before {
    content: "¥";
    font-size: 22rpx;
  }
  /* 复制按钮 */
  .copy {
    display: inline-block;
    width: 92rpx;
    height: 40rpx;
    border-radius: 20rpx;
    color: #01c1a7;
    border: 1rpx solid #01c1a7;
    text-align: center;
    line-height: 40rpx;
    margin-left: 32rpx;
    font-size: 22rpx;
  }
  
  /* 状态按钮 */
  .btn-state {
    width: 504rpx;
    height: 90rpx;
    border-radius: 45rpx;
    font-size: 32rpx;
    text-align: center;
    line-height: 90rpx;
  }
  
  .state-has-state {
    color: #fff;
    background: #dbdbdb;
  }
  
  .state-no-state {
    color: #fff;
    background: #01c1a7;
  }
  
  .common-btn {
    width: 670rpx;
    height: 90rpx;
    border-radius: 8rpx;
    font-size: 32rpx;
    margin: 0 auto;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  /* 布局 */
  .flex-ver {
    display: flex;
    vertical-align: middle;
    justify-content: space-between;
  }
  .flex-between {
    display: flex;
    justify-content: space-between;
    /* align-items: center; */
  }
  .flex-center {
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .flex {
    display: flex;
  }
  .flex-column {
    display: flex;
    flex-direction: column;
  }
  .flex-space {
    flex: 1;
  }
  /* 线条 */
  
  .border-bottom {
    border-bottom: 1rpx solid #d6d6d6;
  }
  
  .radiu-ss {
    border-radius: 8rpx;
  }
  
  /* 文字 */
  
  .text-xs {
    font-size: 22rpx;
  }
  
  .text-xx {
    font-size: 24rpx;
  }
  
  .text-sm {
    font-size: 26rpx;
  }
  
  .text-df {
    font-size: 28rpx;
  }
  
  .text-lg {
    font-size: 30rpx;
  }
  
  .text-xl {
    font-size: 34rpx;
  }
  
  .text-ml {
    font-size: 36rpx;
  }
  
  .text-bold {
    font-weight: bold;
  }
  
  .text-center {
    text-align: center;
  }
  
  .text-weight {
    font-weight: 400;
  }
  
  .weight-sl {
    font-weight: 500;
  }
  
  
  
  .color-green {
    color: #01c1a7;
  }
  
  .color-red {
    color: #df5252;
  }
  
  .color-blc {
    color: #000;
  }
  
  .color-black {
    color: #333;
  }
  
  .color-white {
    color: #fff;
  }
  
  .color-grey {
    color: #999;
  }
  
  /* 背景 */
  
  .bg-green {
    background: #01c1a7;
  }
  
  .bg-grey {
    background: #f7f8f9;
  }
  
  .bg-white {
    background: #fff;
  }
  
  .filterState {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: #000;
    opacity: 0.4;
  }
  
  /* 首页以及消息页列表 */
  .card {
    margin: 12rpx auto;
    width: 718rpx;
    padding: 36rpx 24rpx 30rpx 24rpx;
    box-sizing: border-box;
    background: #fff;
    border-radius: 8rpx;
  }
  
  .card_mid {
    margin: 28rpx 0 24rpx 0;
    color: #999;
    font-size: 26rpx;
  }
  
  .info-salary {
    min-width: 200rpx;
    text-align: right;
  }
  .division {
    width: 1rpx;
    height: 16rpx;
    box-sizing: content-box;
    background: red;
    margin: 0 14rpx;
    background: #d8d8d8;
    display: inline-block;
  }
  
  .user_info {
    display: flex;
    align-items: center;
  }
  .user_img {
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
  }
  
  .user_name {
    margin-left: 16rpx;
  }
  
  .blank {
    width: 20rpx;
    height: 100%;
    display: inline-block;
  }
  
  /* 详情页 */
  .announce_detail {
    padding-bottom: 142rpx;
  }
  .base_info {
    width: 718rpx;
    margin: 16rpx auto;
    margin-bottom: 12rpx;
    background: #fff;
    padding: 36rpx 24rpx 0rpx 24rpx;
    box-sizing: border-box;
  }
  
  .announceNmae {
    color: #333;
    font-size: 44rpx;
    font-weight: 500;
  }
  
  .endTime {
    width: 256rpx;
    height: 40rpx;
    margin-top: 20rpx;
    color: #3dd3bf;
    border-radius: 4rpx;
    border: 1rpx solid #01c1a7;
    font-size: 22rpx;
    box-sizing: border-box；;
  }
  
  .endTime-title {
    width: 104rpx;
    height: 40rpx;
    border-top-left-radius: 4rpx;
    border-bottom-left-radius: 4rpx;
    background: #eafcfa;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  .detail-list {
    width: 100%;
    padding: 40rpx 0;
    border-bottom: 2rpx solid #f2f2f2;
  }
  
  .flex-detail {
    display: flex;
    height: 40rpx;
    justify-content: flex-start;
    align-items: center;
    font-size: 28rpx;
    margin-top: 16rpx;
  }
  
  .flex-detail:nth-child(1) {
    margin-top: 0;
  }
  
  .announce-name-flex {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
  }
  
  .info-salary {
    min-width: 200rpx;
    text-align: right;
  }
  
  .detailImg {
    width: 24rpx;
    height: 24rpx;
    margin-right: 18rpx;
  }
  
  .detail-item {
    width: 140rpx;
    color: #999;
  }
  
  .detail-user {
    height: 96rpx;
    width: 100%;
  }
  
  .detail-user .user {
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    color: #333;
    font-size: 24rpx;
  }
  
  .userimg {
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
    margin-right: 16rpx;
  }
  
  /* 通告明细 */
  
  .detail_info {
    width: 718rpx;
    margin: 16rpx auto;
    background: #fff;
    padding: 36rpx 24rpx 48rpx 24rpx;
    font-size: 28rpx;
    box-sizing: border-box;
  }
  
  .contact-header {
    display: inline-block;
    width: 6rpx;
    height: 24rpx;
    background: #01c1a7;
    border-radius: 3rpx;
    margin-right: 20rpx;
  }
  
  .detail-area {
    font-size: 34rpx;
    font-weight: 500;
    color: #333;
  }
  
  .detail-title {
    margin-bottom: 40rpx;
  }
  
  .contact-detail {
    display: inline-block;
    width: 120rpx;
    color: #999;
  }
  
  .contact-list {
    margin-bottom: 28rpx;
    font-size: 28rpx;
  }
  
  .phonenum {
    margin-bottom: 0;
  }
  
  .detail-blank {
    display: inline-block;
    width: 10rpx;
    height: 34rpx;
  }
  
  /* 报名人数 */
  
  .applynum {
    box-sizing: border-box;
  }
  
  .seeresult {
    display: inline-block;
    margin-right: 12rpx;
  }
  
  .seegroup {
    display: flex;
    flex-wrap: wrap;
  }
  
  .seeuser {
    width: 48rpx;
    height: 48rpx;
    display: inline-block;
    border-radius: 50%;
    margin-right: -5rpx;
  }
  