/* pages/act/detail/detail.wxss */
.announce_detail{
    padding-bottom: 140rpx;
  }
  .base_info{
    width: 718rpx;
    margin: 16rpx auto;
    margin-bottom: 12rpx;
    background: #FFFFFF;
    padding: 36rpx 24rpx 0rpx 24rpx;
    box-sizing:  border-box;
  }
  .info-salary{
    min-width: 200rpx;
    text-align: right;
  }
  .announceNmae{
    color:#333333;
    font-size: 44rpx;
    font-weight: 500;
    flex: 1;
    word-wrap: break-word;
  }
  .endTime{
    width: 256rpx;
    height: 40rpx;
    margin-top: 20rpx;
    color:#3DD3BF;
    border-radius: 4rpx;
    border:1rpx solid #01C1A7;
    font-size: 22rpx;
  }
  .endTime-title{
    width:104rpx;
    height: 40rpx;
    background: #EAFCFA;
    display: flex;
    border-top-left-radius: 4rpx;
    border-bottom-left-radius: 4rpx;
    justify-content: center;
    align-items: center;
  }
  .detail-list{
    width: 100%;
    padding:40rpx 0;
    border-bottom: 2rpx solid #F2F2F2;
  }
  
  .flex-detail{
    display: flex;
    height: 40rpx;
    justify-content: flex-start;
    align-items: center;
    font-size: 28rpx;
    margin-top: 16rpx;
  }
  .flex-detail:nth-child(1) {
    margin-top: 0;
  }
  .detailImg{
    width:24rpx;
    height: 24rpx;
    margin-right: 18rpx;
  }
  
  .detail-item{
    width:140rpx;
    color:#999999;
  }
  .detail-user{
    height: 96rpx;
    width: 100%;
  }
  .detail-user .user{
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    color:#333333;
    font-size: 24rpx;
  }
  .userimg{
    width:48rpx;
    height: 48rpx;
    border-radius: 50%;
    margin-right: 16rpx;
  }
  /* 通告明细 */
  .detail_info{
    width: 718rpx;
    margin: 16rpx auto;
    background: #FFFFFF;
    padding: 36rpx 24rpx 48rpx 24rpx;
    font-size: 28rpx;
    box-sizing: border-box;
  }
  .contact-header{
    display: inline-block;
    width:6rpx;
    height: 24rpx;
    background: #01C1A7;
    border-radius: 3rpx;
    margin-right: 20rpx;
  }
  .detail-area{
    font-size: 34rpx;
    font-weight: 500;
    color:#333333;
  }
  .detail-title{
    margin-bottom: 40rpx;
  }
  .contact-detail{
    display: inline-block;
    width:120rpx;
    color:#999999;
  }
  .contact-list{
    margin-bottom: 28rpx;
    font-size: 28rpx;
  }
  .phonenum{
    margin-bottom: 0;
  }
  .detail-blank{
    display: inline-block;
    width: 10rpx;
    height: 34rpx;
  }
  /* 报名人数 */
  .applynum{
  
    /* margin-bottom: 142rpx;
    margin-bottom: 300rpx; */
    box-sizing: border-box;
  }
  .seeresult{
    display: inline-block;
    margin-right: 12rpx;
  }
  .seegroup{
    display: flex;
    flex-wrap: wrap;
  }
  .seeuser{
    width: 48rpx;
    height: 48rpx;
    display: inline-block;
    border-radius: 50%;
    margin-right: -5rpx;
  }
  /* 底部 */
  .bottom-detail{
    width: 750rpx;
    height: 118rpx;
    position: fixed;
    bottom: 0;
    box-sizing: border-box;
  }
  
  .action{
    width: 750rpx;
    height: 118rpx;
    padding:14rpx 16rpx 14rpx 44rpx;
    display: flex;
    background: #FFFFFF;
    box-sizing: border-box;
  }
  .bottombt{
    height: 100%;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    font-size: 20rpx;
    margin-right: 54rpx;
  }
  .homebtn{
    width:40rpx;
    height: 40rpx;
    padding:4rpx;
    box-sizing: content-box;
  }
  .sharebt{
    height: 100%;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    font-size: 20rpx;
    margin-right: 40rpx;
    position: relative;
    z-index: 10;
  }
  .sharebtn{
    width:43rpx;
    height: 43rpx;
    /* padding:4rpx; */
    box-sizing: content-box;
  }
  .sharebt .openshare{
    position: absolute;
    height: 50rpx;
    width:100%;
    z-index:99;
    opacity: 0.1;
  }
  .openshare::after{
    border:none;
    border-radius: 0;
  }
  